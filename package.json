{
  "name": "@gallolabs/open-metrics",
  "version": "0.1.1",
  "description": "Open metrics",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -Rf dist && tsc",
    "test": "npm run build && npx c8 --all --src 'src' --exclude '*/*test*' --report-dir=/tmp/nyc mocha 'dist/test/*test*.js'",
    "deploy": "npm run test && rm -R dist/test && npm login && trap 'npm logout' EXIT && release-it --no-git.requireCleanWorkingDir"
  },
  "type": "module",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "c8": "^10.1.2",
    "mocha": "^10.2.0",
    "nock": "^13.5.5",
    "release-it": "^17.0.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "workspaces": [
    "."
  ],
  "dependencies": {
    "fastify": "^5.0.0",
    "hot-shots": "^10.2.1",
    "prom-client": "^15.1.3"
  }
}
